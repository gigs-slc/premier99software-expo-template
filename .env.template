# Environment Variables Template
# Copy this file to .env and fill in your values

# MMKV Encryption Key (generate a secure random string for production)
EXPO_PUBLIC_MMKV_ENCRYPTION_KEY=your-secure-encryption-key-here

# Supabase Configuration (optional - only needed if using Supabase)
# EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration (customize for your backend)
# EXPO_PUBLIC_API_URL=https://api.yourapp.com
# EXPO_PUBLIC_API_TIMEOUT=30000

# Feature Flags (optional)
# EXPO_PUBLIC_ENABLE_ANALYTICS=false
# EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false
